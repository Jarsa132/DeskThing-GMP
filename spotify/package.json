{
  "name": "spotify",
  "private": true,
  "version": "0.8.1",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "del /F /Q dist && vite build && npm run build-server && npm run packzip-win",
    "build-server": "tsc --project tsconfig.server.json",
    "packzip": "cd dist && zip -r ../spotify.zip . && cd ..",
    "packzip-win": "cd dist && tar -a -c -f spotify.zip . && cd ..",
    "setup": "node setup.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.6",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "deskthing-client": "^0.7.6",
    "deskthing-server": "^0.7.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.2.0",
    "globals": "^15.9.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
